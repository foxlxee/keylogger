<Window x:Class="ReceivingApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReceivingApp"
        mc:Ignorable="d"
        x:ClassModifier="internal"
        WindowStartupLocation="CenterScreen" Height="430" Width="410" Background="#FFFDFDFD">
    <ScrollViewer x:Name="sv" Focusable="False" FocusVisualStyle="{x:Null}">
        <StackPanel x:Name="sp"/>
    </ScrollViewer>
</Window>